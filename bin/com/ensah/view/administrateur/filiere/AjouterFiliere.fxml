									layoutY="39.0" prefHeight="25.0" prefWidth="274.0"
									promptText="Professeur" />
								<Label fx:id="professeurMsg" layoutX="743.0"
									layoutY="40.0" text="professeurMsg" textFill="RED" />
							</children>
						</Pane>
						<Pane prefHeight="135.0" prefWidth="900.0">
							<children>
								<Button fx:id="ajouterFiliereBtn" layoutX="345.0"
									layoutY="48.0" mnemonicParsing="false"
									onAction="#ajouterFiliere" prefHeight="25.0" prefWidth="189.0"
									text="Ajouter" />
							</children>
						</Pane>
					</children>
				</VBox>
			</center>
